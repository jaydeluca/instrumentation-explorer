{
  "id": "spring-web-3.1",
  "library_group": "spring",
  "description": "This instrumentation provides a library integration that enables capturing HTTP client spans and metrics for Spring's RestTemplate.\n",
  "library_link": "https://github.com/spring-projects/spring-framework",
  "source_path": "instrumentation/spring/spring-web/spring-web-3.1",
  "scope": {
    "name": "io.opentelemetry.spring-web-3.1",
    "schema_url": "https://opentelemetry.io/schemas/1.37.0"
  },
  "javaagent_target_versions": [
    "org.springframework:spring-web:[3.1.0.RELEASE,6)"
  ],
  "telemetry": {
    "default": {
      "metrics": [
        {
          "name": "http.client.request.duration",
          "description": "Duration of HTTP client requests.",
          "type": "HISTOGRAM",
          "unit": "s",
          "attributes": [
            {
              "name": "http.request.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.response.status_code",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "server.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.port",
              "type": "LONG",
              "semconv": true
            }
          ]
        }
      ],
      "spans": [
        {
          "span_kind": "CLIENT",
          "attributes": [
            {
              "name": "error.type",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.request.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.response.status_code",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "server.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.port",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "url.full",
              "type": "STRING",
              "semconv": true
            }
          ]
        }
      ]
    }
  },
  "markdown_hash": "9afbf0f82c56",
  "markdown_url": "/data/markdown/spring-web-3.1-9afbf0f82c56.md"
}