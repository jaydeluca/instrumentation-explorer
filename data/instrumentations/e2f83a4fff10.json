{
  "id": "lettuce-5.1",
  "library_group": "lettuce",
  "description": "This instrumentation enables database client spans and database client metrics for the Lettuce Redis client.\n",
  "library_link": "https://github.com/redis/lettuce",
  "source_path": "instrumentation/lettuce/lettuce-5.1",
  "semantic_conventions": [
    "DATABASE_CLIENT_SPANS",
    "DATABASE_CLIENT_METRICS"
  ],
  "scope": {
    "name": "io.opentelemetry.lettuce-5.1"
  },
  "target_versions": {
    "javaagent": [
      "io.lettuce:lettuce-core:[5.1.0.RELEASE,)"
    ],
    "library": [
      "io.lettuce:lettuce-core:5.1.0.RELEASE"
    ]
  },
  "configurations": [
    {
      "name": "otel.instrumentation.lettuce.experimental.command-encoding-events.enabled",
      "description": "Enables capturing `redis.encode.start` and `redis.encode.end` span events.",
      "type": "boolean",
      "default": false
    },
    {
      "name": "otel.instrumentation.common.db-statement-sanitizer.enabled",
      "description": "Enables statement sanitization for database queries.",
      "type": "boolean",
      "default": true
    }
  ],
  "telemetry": {
    "default": {
      "metrics": [
        {
          "name": "db.client.operation.duration",
          "description": "Duration of database client operations.",
          "type": "HISTOGRAM",
          "unit": "s",
          "attributes": [
            {
              "name": "db.system.name",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "network.peer.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "network.peer.port",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "server.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.port",
              "type": "LONG",
              "semconv": true
            }
          ]
        }
      ],
      "spans": [
        {
          "span_kind": "CLIENT",
          "attributes": [
            {
              "name": "db.query.text",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "db.system.name",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "error",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "network.peer.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "network.peer.port",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "network.type",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.port",
              "type": "LONG",
              "semconv": true
            }
          ]
        }
      ]
    }
  }
}