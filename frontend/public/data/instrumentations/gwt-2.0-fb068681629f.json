{
  "id": "gwt-2.0",
  "display_name": "GWT",
  "library_group": "gwt",
  "description": "This instrumentation enables RPC server spans for GWT RPC requests.",
  "library_link": "https://www.gwtproject.org/",
  "source_path": "instrumentation/gwt-2.0",
  "semantic_conventions": [
    "RPC_SERVER_SPANS"
  ],
  "scope": {
    "name": "io.opentelemetry.gwt-2.0"
  },
  "telemetry": {
    "default": {
      "metrics": [],
      "spans": [
        {
          "span_kind": "SERVER",
          "attributes": [
            {
              "name": "rpc.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "rpc.service",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "rpc.system",
              "type": "STRING",
              "semconv": true
            }
          ]
        }
      ]
    }
  }
}