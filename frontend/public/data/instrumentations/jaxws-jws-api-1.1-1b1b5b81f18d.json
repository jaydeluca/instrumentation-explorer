{
  "id": "jaxws-jws-api-1.1",
  "display_name": "JWS API",
  "library_group": "jaxws",
  "description": "This instrumentation enables controller spans for methods annotated with the @WebService annotation from the JWS API (controller spans are disabled by default).\n",
  "library_link": "https://github.com/jakartaee/jws-api",
  "source_path": "instrumentation/jaxws/jaxws-jws-api-1.1",
  "features": [
    "CONTROLLER_SPANS"
  ],
  "disabled_by_default": true,
  "scope": {
    "name": "io.opentelemetry.jaxws-jws-api-1.1"
  },
  "configurations": [
    {
      "name": "otel.instrumentation.common.experimental.controller-telemetry.enabled",
      "description": "Enables the creation of experimental controller spans.",
      "type": "boolean",
      "default": false
    }
  ],
  "telemetry": {
    "otel.instrumentation.common.experimental.controller-telemetry.enabled=true": {
      "metrics": [],
      "spans": [
        {
          "span_kind": "INTERNAL",
          "attributes": [
            {
              "name": "code.function",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "code.namespace",
              "type": "STRING",
              "semconv": true
            }
          ]
        }
      ]
    }
  }
}