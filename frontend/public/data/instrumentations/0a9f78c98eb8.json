{
  "id": "async-http-client-1.9",
  "library_group": "async",
  "description": "This instrumentation enables CLIENT spans and metrics for version 1 of the AsyncHttpClient (AHC) HTTP client.",
  "source_path": "instrumentation/async-http-client/async-http-client-1.9",
  "scope": {
    "name": "io.opentelemetry.async-http-client-1.9"
  },
  "target_versions": {
    "javaagent": [
      "com.ning:async-http-client:[1.9.0,)"
    ]
  },
  "telemetry": {
    "default": {
      "metrics": [
        {
          "name": "http.client.request.duration",
          "description": "Duration of HTTP client requests.",
          "type": "HISTOGRAM",
          "unit": "s",
          "attributes": [
            {
              "name": "http.request.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.response.status_code",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "server.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.port",
              "type": "LONG",
              "semconv": true
            }
          ]
        }
      ],
      "spans": [
        {
          "span_kind": "CLIENT",
          "attributes": [
            {
              "name": "error.type",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.request.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.request.method_original",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.response.status_code",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "server.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.port",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "url.full",
              "type": "STRING",
              "semconv": true
            }
          ]
        }
      ]
    }
  }
}