{
  "id": "nats-2.17",
  "display_name": "NATS Client",
  "library_group": "nats",
  "description": "This instrumentation enables messaging spans for NATS message producers and consumers.",
  "library_link": "https://nats.io/",
  "source_path": "instrumentation/nats/nats-2.17",
  "semantic_conventions": [
    "MESSAGING_SPANS"
  ],
  "scope": {
    "name": "io.opentelemetry.nats-2.17"
  },
  "target_versions": {
    "javaagent": [
      "io.nats:jnats:[2.17.2,)"
    ],
    "library": [
      "io.nats:jnats:2.17.2"
    ]
  },
  "configurations": [
    {
      "name": "otel.instrumentation.messaging.experimental.capture-headers",
      "description": "Enables capturing messaging headers as span attributes. Provide a comma-separated list of header names to capture.\n",
      "type": "list",
      "default": ""
    }
  ],
  "telemetry": {
    "default": {
      "metrics": [],
      "spans": [
        {
          "span_kind": "CONSUMER",
          "attributes": [
            {
              "name": "messaging.client_id",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "messaging.destination.name",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "messaging.destination.temporary",
              "type": "BOOLEAN",
              "semconv": true
            },
            {
              "name": "messaging.header.captured_header",
              "type": "STRING_ARRAY",
              "semconv": false
            },
            {
              "name": "messaging.message.body.size",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "messaging.operation",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "messaging.system",
              "type": "STRING",
              "semconv": true
            }
          ]
        },
        {
          "span_kind": "PRODUCER",
          "attributes": [
            {
              "name": "messaging.client_id",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "messaging.destination.name",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "messaging.destination.temporary",
              "type": "BOOLEAN",
              "semconv": true
            },
            {
              "name": "messaging.header.captured_header",
              "type": "STRING_ARRAY",
              "semconv": false
            },
            {
              "name": "messaging.message.body.size",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "messaging.operation",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "messaging.system",
              "type": "STRING",
              "semconv": true
            }
          ]
        }
      ]
    }
  }
}