{
  "id": "aws-lambda-events-2.2",
  "library_group": "aws",
  "description": "Provides full instrumentation of the Lambda library, including standard and custom event types, from `aws-lambda-java-events` 2.2+.\n",
  "source_path": "instrumentation/aws-lambda/aws-lambda-events-2.2",
  "scope": {
    "name": "io.opentelemetry.aws-lambda-events-2.2"
  },
  "target_versions": {
    "javaagent": [
      "com.amazonaws:aws-lambda-java-core:[1.0.0,)"
    ],
    "library": [
      "com.amazonaws:aws-lambda-java-events:2.2.1",
      "com.amazonaws:aws-lambda-java-core:1.0.0"
    ]
  },
  "configurations": [
    {
      "name": "otel.instrumentation.aws-lambda.flush-timeout",
      "description": "Flush timeout in milliseconds.",
      "type": "int",
      "default": 10000
    }
  ],
  "telemetry": {
    "default": {
      "metrics": [],
      "spans": [
        {
          "span_kind": "CONSUMER",
          "attributes": [
            {
              "name": "messaging.operation",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "messaging.system",
              "type": "STRING",
              "semconv": true
            }
          ]
        },
        {
          "span_kind": "SERVER",
          "attributes": [
            {
              "name": "faas.invocation_id",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "faas.trigger",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "http.request.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.response.status_code",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "url.full",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "user_agent.original",
              "type": "STRING",
              "semconv": true
            }
          ]
        }
      ]
    }
  }
}