{
  "id": "redisson-3.0",
  "library_group": "redisson",
  "description": "This instrumentation enables database client spans and database client metrics for Redisson Redis client operations.",
  "library_link": "https://github.com/redisson/redisson",
  "source_path": "instrumentation/redisson/redisson-3.0",
  "semantic_conventions": [
    "DATABASE_CLIENT_SPANS",
    "DATABASE_CLIENT_METRICS"
  ],
  "scope": {
    "name": "io.opentelemetry.redisson-3.0"
  },
  "javaagent_target_versions": [
    "org.redisson:redisson:[3.0.0,3.17.0)"
  ],
  "telemetry": {
    "default": {
      "metrics": [],
      "spans": [
        {
          "span_kind": "CLIENT",
          "attributes": [
            {
              "name": "db.operation",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "db.statement",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "db.system",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "network.peer.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "network.peer.port",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "network.type",
              "type": "STRING",
              "semconv": true
            }
          ]
        }
      ]
    }
  }
}