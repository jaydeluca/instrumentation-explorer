{
  "id": "armeria-1.3",
  "library_group": "armeria",
  "description": "This instrumentation enables CLIENT and SERVER spans and metrics for the Armeria HTTP client and server.",
  "source_path": "instrumentation/armeria/armeria-1.3",
  "scope": {
    "name": "io.opentelemetry.armeria-1.3"
  },
  "target_versions": {
    "javaagent": [
      "com.linecorp.armeria:armeria:[1.3.0,)"
    ],
    "library": [
      "com.linecorp.armeria:armeria:1.3.0"
    ]
  },
  "telemetry": {
    "default": {
      "metrics": [
        {
          "name": "http.client.request.duration",
          "description": "Duration of HTTP client requests.",
          "type": "HISTOGRAM",
          "unit": "s",
          "attributes": [
            {
              "name": "http.request.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.response.status_code",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "network.protocol.version",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.port",
              "type": "LONG",
              "semconv": true
            }
          ]
        },
        {
          "name": "http.server.request.duration",
          "description": "Duration of HTTP server requests.",
          "type": "HISTOGRAM",
          "unit": "s",
          "attributes": [
            {
              "name": "http.request.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.response.status_code",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "http.route",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "network.protocol.version",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "url.scheme",
              "type": "STRING",
              "semconv": true
            }
          ]
        }
      ],
      "spans": [
        {
          "span_kind": "CLIENT",
          "attributes": [
            {
              "name": "error.type",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.request.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.response.status_code",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "network.peer.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "network.peer.port",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "network.protocol.version",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.port",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "url.full",
              "type": "STRING",
              "semconv": true
            }
          ]
        },
        {
          "span_kind": "SERVER",
          "attributes": [
            {
              "name": "client.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "error.type",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.request.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.response.status_code",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "http.route",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "network.peer.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "network.peer.port",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "network.protocol.version",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.port",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "url.path",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "url.query",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "url.scheme",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "user_agent.original",
              "type": "STRING",
              "semconv": true
            }
          ]
        }
      ]
    }
  }
}