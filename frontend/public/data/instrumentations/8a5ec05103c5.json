{
  "id": "couchbase-3.4",
  "library_group": "couchbase",
  "description": "Couchbase instrumentation is owned by the Couchbase project for versions 3+. This instrumentation automatically configures the instrumentation provided by the Couchbase library.\n",
  "library_link": "https://github.com/couchbase/couchbase-java-client",
  "source_path": "instrumentation/couchbase/couchbase-3.4",
  "semantic_conventions": [
    "DATABASE_CLIENT_SPANS"
  ],
  "features": [
    "AUTO_INSTRUMENTATION_SHIM"
  ],
  "scope": {
    "name": "io.opentelemetry.couchbase-3.4"
  },
  "target_versions": {
    "javaagent": [
      "com.couchbase.client:java-client:[3.4.0,)"
    ]
  },
  "telemetry": {
    "default": {
      "metrics": [],
      "spans": [
        {
          "span_kind": "CLIENT",
          "attributes": [
            {
              "name": "db.couchbase.collection",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "db.couchbase.document_id",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "db.couchbase.local_id",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "db.couchbase.operation_id",
              "type": "LONG",
              "semconv": false
            },
            {
              "name": "db.couchbase.retries",
              "type": "LONG",
              "semconv": false
            },
            {
              "name": "db.couchbase.scope",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "db.couchbase.server_duration",
              "type": "LONG",
              "semconv": false
            },
            {
              "name": "db.couchbase.service",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "db.name",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "db.operation",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "db.system",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "net.host.name",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "net.host.port",
              "type": "LONG",
              "semconv": false
            },
            {
              "name": "net.peer.name",
              "type": "STRING",
              "semconv": false
            },
            {
              "name": "net.peer.port",
              "type": "LONG",
              "semconv": false
            },
            {
              "name": "net.transport",
              "type": "STRING",
              "semconv": false
            }
          ]
        }
      ]
    }
  }
}