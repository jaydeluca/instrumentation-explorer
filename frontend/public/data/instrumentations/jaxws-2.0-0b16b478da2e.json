{
  "id": "jaxws-2.0",
  "display_name": "JAX-WS",
  "library_group": "jaxws",
  "description": "This instrumentation enables controller spans for JAX-WS Provider implementations (controller spans are disabled by default).\n",
  "library_link": "https://github.com/jakartaee/jax-ws-api",
  "source_path": "instrumentation/jaxws/jaxws-2.0",
  "features": [
    "CONTROLLER_SPANS"
  ],
  "scope": {
    "name": "io.opentelemetry.jaxws-2.0"
  },
  "javaagent_target_versions": [
    "javax.xml.ws:jaxws-api:[2.0,]"
  ],
  "configurations": [
    {
      "name": "otel.instrumentation.common.experimental.controller-telemetry.enabled",
      "description": "Enables the creation of experimental controller spans.",
      "type": "boolean",
      "default": false
    }
  ],
  "telemetry": {
    "otel.instrumentation.common.experimental.controller-telemetry.enabled=true": {
      "metrics": [],
      "spans": [
        {
          "span_kind": "INTERNAL",
          "attributes": [
            {
              "name": "code.function",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "code.namespace",
              "type": "STRING",
              "semconv": true
            }
          ]
        }
      ]
    }
  }
}