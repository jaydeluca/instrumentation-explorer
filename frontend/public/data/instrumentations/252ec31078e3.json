{
  "id": "apache-httpclient-4.3",
  "display_name": "Apache HttpClient",
  "library_group": "apache",
  "description": "This instrumentation provides a library integration that enables HTTP client spans and HTTP client metrics for the Apache HttpClient.",
  "library_link": "https://hc.apache.org/index.html",
  "source_path": "instrumentation/apache-httpclient/apache-httpclient-4.3",
  "semantic_conventions": [
    "HTTP_CLIENT_SPANS",
    "HTTP_CLIENT_METRICS"
  ],
  "scope": {
    "name": "io.opentelemetry.apache-httpclient-4.3"
  },
  "target_versions": {
    "library": [
      "org.apache.httpcomponents:httpclient:[4.3,4.+)"
    ]
  },
  "telemetry": {
    "default": {
      "metrics": [
        {
          "name": "http.client.request.duration",
          "description": "Duration of HTTP client requests.",
          "type": "HISTOGRAM",
          "unit": "s",
          "attributes": [
            {
              "name": "http.request.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.response.status_code",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "network.protocol.version",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.port",
              "type": "LONG",
              "semconv": true
            }
          ]
        }
      ],
      "spans": [
        {
          "span_kind": "CLIENT",
          "attributes": [
            {
              "name": "error.type",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.request.method",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.request.method_original",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "http.request.resend_count",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "http.response.status_code",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "network.protocol.version",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.address",
              "type": "STRING",
              "semconv": true
            },
            {
              "name": "server.port",
              "type": "LONG",
              "semconv": true
            },
            {
              "name": "url.full",
              "type": "STRING",
              "semconv": true
            }
          ]
        }
      ]
    }
  },
  "markdown_hash": "b5c321e75d2b",
  "markdown_url": "/data/markdown/b5c321e75d2b.md"
}